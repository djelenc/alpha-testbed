<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<shortcuts>

  <skipIfNotSupported/>

  <programGroup defaultName="Alpha Testbed" location="StartMenu"/>
  
  <shortcut
     name="Run Alpha Testbed"
     programGroup="yes"
     desktop="no"
     applications="no"
     startMenu="no"
     startup="no"
     target="$INSTALL_PATH/start_model.command"
     commandLine=""
     workingDirectory="$INSTALL_PATH"
     description="Model"
     iconFile="$INSTALL_PATH/testbed/icons/model.png"
     iconIndex="0"
     type="Application"
     encoding="UTF-8"
     terminal="true"
     KdeSubstUID="false"
     initialState="normal">   
     <createForPack name="Model"/>
  </shortcut>

  <shortcut
     name="Homepage of Alpha Testbed"
     programGroup="yes"
     desktop="yes"
     applications="no"
     startMenu="no"
     startup="no"
     target="http://atb.fri.uni-lj.si"
     workingDirectory="$INSTALL_PATH"
     commandLine=""
     initialState="noShow"
     iconFile="help"
     iconIndex="0" 
     url="http://atb.fri.uni-lj.si"                                
     type="Link"
     encoding="UTF-8" 
   	 createForAll="true"  
     description="Go to the homepage of ATB">
     <createForPack name="Model"/>
  </shortcut>
   
  <shortcut
     name="Uninstall Alpha Testbed"
     programGroup="yes"
     desktop="no"
     applications="no"
     startMenu="no"
     startup="no"
     target="java"
     commandLine="-jar &quot;$INSTALL_PATH/Uninstaller/uninstaller.jar&quot;"
     initialState="noShow"
     iconFile="trashcan_full"
     iconIndex="0" 
     workingDirectory="$INSTALL_PATH/Uninstaller"
     type="Application"
     encoding="UTF-8"
     KdeSubstUID="true"
     KdeUsername="root"
     createForAll="false"
	 Categories="Development;IDE" 
	 TryExec="java -jar &quot;$INSTALL_PATH/Uninstaller/uninstaller.jar&quot;"
     description="This Removes the Model">
     <createForPack name="Model" />
  </shortcut>
   
</shortcuts>