<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<shortcuts>

  <skipIfNotSupported/>
  
  <programGroup defaultName="Alpha Testbed" location="StartMenu"/>
  
  <shortcut
     name="Run Alpha Testbed"
     programGroup="yes"
     desktop="no"
     applications="no"
     startMenu="no"
     startup="no"
     target="$INSTALL_PATH\start_model.bat"
     commandLine=""
     workingDirectory="$INSTALL_PATH"
     description="Model"
     iconFile="$INSTALL_PATH\testbed\icons\model.bmp"
     iconIndex="0"
     initialState="noShow">
     <createForPack name="Model"/>
  </shortcut>
  
  <shortcut
     name="Homepage of Alpha Testbed"
     programGroup="yes"
     desktop="no"
     applications="no"
     startMenu="no"
     startup="no"
     target="http://atb.fri.uni-lj.si"
     commandLine=""
	 iconFile="%SystemRoot%\system32\SHELL32.dll"
     iconIndex="23" 
     workingDirectory="$INSTALL_PATH"
     url="http://atb.fri.uni-lj.si"                                
     type="Link"
     description="Go to the homepage of ATB">
     <createForPack name="Model" />
  </shortcut>
  
  <shortcut
     name="Uninstall Alpha Testbed"
     programGroup="yes"
     desktop="no"
     applications="no"
     startMenu="no"
     startup="no"
     target="$INSTALL_PATH\Uninstaller\uninstaller.jar"     
     commandLine=""
     workingDirectory="$INSTALL_PATH\Uninstaller"
	 iconFile="%SystemRoot%\system32\SHELL32.dll"
     iconIndex="31" 
     description="This Removes the Alpha Testbed">
     <createForPack name="Model" />
   </shortcut>
   
</shortcuts>